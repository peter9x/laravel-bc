{
    "name": "peter9x/laravel-bc",
    "description": "Laravel package for Microsoft Business Central API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter",
            "email": "your-email@example.com"
        }
    ],
    "homepage": "https://github.com/peter9x/laravel-bc",
    "keywords": [
        "Laravel",
        "Business Central",
        "Microsoft Business Central",
        "Microsoft",
        "BC"
    ],
    "require": {
        "illuminate/support": "^10.0|^11.0|^12.0",
        "league/oauth2-client": "^2",
        "guzzlehttp/guzzle": "7.x",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "pestphp/pest": "^v2.34|^3.7"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "autoload": {
        "psr-4": {
            "Mupy\\BusinessCentral\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mupy\\BusinessCentral\\BusinessCentralServiceProvider"
            ],
            "aliases": {
                "BusinessCentral": "Mupy\\BusinessCentral\\Facades\\BusinessCentral"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}